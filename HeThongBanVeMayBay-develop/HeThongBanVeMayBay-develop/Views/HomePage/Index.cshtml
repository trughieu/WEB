<body>

    <!-- Navbar Section -->
    <header>
        <div class="container-fluid navbar-container">
            <nav class="navbar navbar-expand-lg navbar-light bg-transparent">
                <a class="navbar-brand" href="index.html">
                    <img src="~/AssetsHome/images/logo-image.jpg" width="150" height="70" class="d-inline-block align-top" alt="" loading="lazy">
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item mr-4 active">
                            <a class="nav-link" href="~/HomePage/Index">Home <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item mr-4">
                            <a class="nav-link" href="~/HomePage/Service">Our Services</a>
                        </li>
                        <li class="nav-item mr-4">
                            <a class="nav-link" href="~/HomePage/About">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/HomePage/Contact">Contact Us</a>
                        </li>
                    </ul>
